                    		</p1:GCheck.layoutData>
                    	</p1:GCheck>
                    	<Label/>
                    	<p1:GCheck description="Enable this option will enable the trigger for the background request source." manifestObj="true" text="Enable trigger" toolTipText="Setting of the bitfield BRSMR.ENTR" widgetName="gcheck_background_trigger_enable">
                    		<p1:GCheck.layoutData>
                    			<GridData verticalIndent="5"/>
                    		</p1:GCheck.layoutData>
                    	</p1:GCheck>
                    	<Label/>
                    	<p1:GCheck description="Enable this option will enable the timer mode for background request source." manifestObj="true" text="Enable timer mode" toolTipText="Setting of the bitfield BRSCTRL.TMEN" widgetName="gcheck_background_timer_mode">
                    		<p1:GCheck.layoutData>
                    			<GridData verticalIndent="5"/>
                    		</p1:GCheck.layoutData>
                    	</p1:GCheck>
                    	<Label/>
                    </p1:GGroup>
       		</p1:Tab>
       	</p1:GTab.control>
       </p1:GTab>
