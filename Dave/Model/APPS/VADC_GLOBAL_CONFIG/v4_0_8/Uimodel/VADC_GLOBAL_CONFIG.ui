       			<p1:GGroup manifestObj="true" text="Background Settings" widgetName="gGroup_background_config" bounds="0, 0, 583, 296">
                    	<p1:GGroup.layout>
                    		<GridLayout marginTop="5" numColumns="2"/>
                    	</p1:GGroup.layout>
                    	<p1:GLabel manifestObj="true" text="Trigger edge selection:" toolTipText="Select Required Edge Signal" widgetName="glabel_background_trigger_edge_sel">
                    		<p1:GLabel.layoutData>
                    			<GridData widthHint="219"/>
                    		</p1:GLabel.layoutData>
                    	</p1:GLabel>
                    	<p1:GCombo description="Configures the Trigger edge of queue request source.&#13;&#10;&lt;UL&gt;&#13;&#10;&lt;LI&gt;No External Trigger: To ignore the Trigger signal. &lt;/LI&gt;&#13;&#10;&lt;LI&gt;External Trigger Upon Rising Edge : To trigger the queue conversion on a rising edge.&lt;/LI&gt;&#13;&#10;&lt;LI&gt;External Trigger Upon Falling Edge :  To trigger the queue conversion on a falling edge .&lt;/LI&gt;&#13;&#10;&lt;LI&gt;External Trigger Upon Both The Edge: To trigger the queue conversion on both rising and falling edges.&lt;/LI&gt;" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="No External Trigger&#13;&#10;External Trigger Upon Rising Edge&#13;&#10;External Trigger Upon Falling Edge&#13;&#10;External Trigger Upon Both The Edge" toolTipText="Setting of the bitfield GxBRSCTRL.XTMODE" value="No External Trigger" widgetName="gcombo_background_trigger_edge_sel">
                    		<p1:GCombo.layoutData>
                    			<GridData grabExcessHorizontalSpace="true" horizontalAlignment="FILL"/>
                    		</p1:GCombo.layoutData>
                    	</p1:GCombo>
                    	<p1:GLabel manifestObj="true" text="Gating selection:" toolTipText="Select the required gating mode" widgetName="glabel_background_gating_sel"/>
                    	<p1:GCombo description="Configures the Gating mode for the queue request source.&#13;&#10;&lt;UL&gt;&#13;&#10;&lt;LI&gt; No Conversion Request is Issued: All conversion requests are blocked.&lt;/LI&gt;&#13;&#10;&lt;LI&gt;All Conversion Requests are Issued: All conversion requests are issued if there are valid entries present in the queue.&lt;/LI&gt;&#13;&#10;&lt;LI&gt;Conversion Request Issued When Gate is High: Conversion requests are issued only when gate level is high.&lt;/LI&gt;&#13;&#10;&lt;LI&gt;Conversion Request Issued When Gate is Low: Conversion requests are issued only when gate level is low.&lt;/LI&gt;" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="No Conversion Request is Issued&#13;&#10;All Conversion Requests are Issued&#13;&#10;Conversion Request Issued When Gate is High&#13;&#10;Conversion Request Issued When Gate is Low" toolTipText="Setting of the bitfield BRSMR.ENGT" value="All Conversion Requests are Issued" widgetName="gcombo_background_gating_sel">
                    		<p1:GCombo.layoutData>
                    			<GridData grabExcessHorizontalSpace="true" horizontalAlignment="FILL"/>
                    		</p1:GCombo.layoutData>
                    	</p1:GCombo>
                    	<p1:GLabel manifestObj="true" text="Priority of background source:" toolTipText="Select the priority" widgetName="glabel_background_priority"/>
                    	<p1:GCombo description="This is Arbitration priority of queue request source.&#13;&#10;Priority-0 is the lowest priority and Priority-3 is the highest.&#13;&#10;This determines the request source which is selected during the arbitration round. Range : 0x0 to 0x3 " mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="Priority-0  (Lowest Priority)&#13;&#10;Priority-1&#13;&#10;Priority-2&#13;&#10;Priority-3 (Highest Priority)&#13;&#10;" toolTipText="Setting of the bitfield GxARBPR.PRIO2." value="Priority-0  (Lowest Priority)" widgetName="gcombo_background_priority">
                    		<p1:GCombo.layoutData>
                    			<GridData grabExcessHorizontalSpace="true" horizontalAlignment="FILL" verticalAlignment="FILL"/>
                    		</p1:GCombo.layoutData>
                    	</p1:GCombo>
                    	<p1:GLabel manifestObj="true" text="Conversion start mode:" toolTipText="Select the required start mode" widgetName="glabel_background_startmode"/>
                    	<p1:GCombo description="Selecting start mode starts the conversion.&#13;&#10;&lt;UL&gt;&#13;&#10;&lt;LI&gt;Wait For Start Mode: In this mode the converter will finish the on-going conversion  and then take up any channel that is pending conversion.&lt;/LI&gt;&#13;&#10;&lt;LI&gt;Cancel Inject Repeat Mode: In this mode the converter will stall the on-going conversion, if a channel is pending in a higher priority request source. After the higher priority request source finishes the conversion the channel which was pre-empted will resume. &lt;/LI&gt;&#13;&#10;&lt;LI&gt;Cancel Inject With Cancel Mode: In this mode the converter will cancel the on-going conversion, if a channel is pending in a higher priority request source.  This cancelled channel will not take part in the current arbitration round. &lt;/LI&gt;" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="Wait For Start Mode&#13;&#10;Cancel Inject Repeat Mode&#13;&#10;" toolTipText="Setting of the bitfield GxARBPR.CSM2." value="Wait For Start Mode" widgetName="gcombo_background_startmode">
                    		<p1:GCombo.layoutData>
                    			<GridData grabExcessHorizontalSpace="true" horizontalAlignment="FILL"/>
                    		</p1:GCombo.layoutData>
                    	</p1:GCombo>
                    	<p1:GLabel manifestObj="true" text="Background specific result register:" toolTipText="Select the required start mode" widgetName="glabel_background_srcreg"/>
                    	<p1:GCombo description="Selecting start mode starts the conversion.&#13;&#10;&lt;UL&gt;&#13;&#10;&lt;LI&gt;Wait For Start Mode: In this mode the converter will finish the on-going conversion  and then take up any channel that is pending conversion.&lt;/LI&gt;&#13;&#10;&lt;LI&gt;Cancel Inject Repeat Mode: In this mode the converter will stall the on-going conversion, if a channel is pending in a higher priority request source. After the higher priority request source finishes the conversion the channel which was pre-empted will resume. &lt;/LI&gt;&#13;&#10;&lt;LI&gt;Cancel Inject With Cancel Mode: In this mode the converter will cancel the on-going conversion, if a channel is pending in a higher priority request source.  This cancelled channel will not take part in the current arbitration round. &lt;/LI&gt;" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="No Specific Register&#13;&#10;GxRES0&#13;&#10;GxRES1&#13;&#10;GxRES2&#13;&#10;GxRES3&#13;&#10;GxRES4&#13;&#10;GxRES5&#13;&#10;GxRES6&#13;&#10;GxRES7&#13;&#10;GxRES8&#13;&#10;GxRES9&#13;&#10;GxRES10&#13;&#10;GxRES11&#13;&#10;GxRES12&#13;&#10;GxRES13&#13;&#10;GxRES14&#13;&#10;GxRES15" toolTipText="Setting of the bitfield BRSCTRL.SRCRESREG" value="No Specific Register" widgetName="gcombo_background_srcreg">
                    		<p1:GCombo.layoutData>
                    			<GridData grabExcessHorizontalSpace="true" horizontalAlignment="FILL"/>
                    		</p1:GCombo.layoutData>
                    	</p1:GCombo>
                    	<p1:GCheck description="Configure the background request source with the properties selected in this tab.&#13;&#10;If unchecked the code will not be generated for the background request source." manifestObj="true" text="Enable background request source initialization" toolTipText="Configure the background request source with the properties selected in this tab." widgetName="gcheck_background_init" value="true"/>
                    	<Label/>
                    	<p1:GCheck description="Enable this option to do a continuous conversion for all the available measurements.&#13;&#10;&lt;UL&gt;&#13;&#10;&lt;LI&gt;Enables the continuous conversion (Autoscan) feature. &#13;&#10;&lt;LI&gt;The conversion of the last channel in the sequence will cause a load event to occur. &lt;LI&gt;Causes continuous conversion of the background scan sequence once it has started." manifestObj="true" text="Enable autoscan" toolTipText="Setting of the bitfield BRSMR.SCAN" widgetName="gcheck_background_autoscan">
                    		<p1:GCheck.layoutData>
                    			<GridData verticalIndent="5"/>
                    		</p1:GCheck.layoutData>
                    	</p1:GCheck>
                    	<Label/>
                    	<p1:GCheck description="Enable this option will enable the background request source interrupt." manifestObj="true" text="Enable request source interrupt" toolTipText="Enable the request source interrupt." widgetName="gcheck_background_req_src_intr">
                    		<p1:GCheck.layoutData>
